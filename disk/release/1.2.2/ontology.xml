    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://purl.org/dc/elements/1.1/creator -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/creator"/>
    


    <!-- http://purl.org/dc/terms/abstract -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/abstract"/>
    


    <!-- http://purl.org/dc/terms/license -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/license"/>
    


    <!-- http://purl.org/dc/terms/title -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/title"/>
    


    <!-- http://purl.org/vocab/vann/preferredNamespacePrefix -->


    <owl:AnnotationProperty rdf:about="http://purl.org/vocab/vann/preferredNamespacePrefix"/>
    


    <!-- http://purl.org/vocab/vann/preferredNamespaceUri -->


    <owl:AnnotationProperty rdf:about="http://purl.org/vocab/vann/preferredNamespaceUri"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.w3.org/2001/XMLSchema#date -->


    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#date"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://disk-project.org/ontology/disk#createdFrom -->


    <owl:ObjectProperty rdf:about="http://disk-project.org/ontology/disk#createdFrom">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/prov#wasDerivedFrom"/>
        <rdfs:domain rdf:resource="http://disk-project.org/ontology/disk#Data"/>
        <rdfs:comment xml:lang="en">Property that states from where these data have been created (e-g-, workflow, metaworkflow, line of inquiry, etc.)</rdfs:comment>
        <rdfs:label xml:lang="en">created from</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://disk-project.org/ontology/disk#hasAuthor -->


    <owl:ObjectProperty rdf:about="http://disk-project.org/ontology/disk#hasAuthor">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://disk-project.org/ontology/disk#Hypothesis"/>
                    <rdf:Description rdf:about="http://disk-project.org/ontology/disk#LineOfInquiry"/>
                    <rdf:Description rdf:about="http://disk-project.org/ontology/disk#TriggeredLineOfInquiry"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://disk-project.org/ontology/disk#Person"/>
        <rdfs:comment xml:lang="en">Property to describe the author of a Line of Inquiry, Triggered Line of Inquiry or Hypothesis</rdfs:comment>
        <rdfs:label xml:lang="en">has author</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://disk-project.org/ontology/disk#hasConfidenceReport -->


    <owl:ObjectProperty rdf:about="http://disk-project.org/ontology/disk#hasConfidenceReport">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://disk-project.org/ontology/disk#Hypothesis"/>
                    <rdf:Description rdf:about="http://www.w3.org/1999/02/22-rdf-syntax-ns#Statement"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://disk-project.org/ontology/disk#ConfidenceReport"/>
        <rdfs:comment xml:lang="en">has confidence report</rdfs:comment>
        <rdfs:label xml:lang="en">Property that links the hypothesis or statement to the ConfidenceReport that supports them</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://disk-project.org/ontology/disk#hasExecution -->


    <owl:ObjectProperty rdf:about="http://disk-project.org/ontology/disk#hasExecution">
        <rdfs:domain rdf:resource="http://disk-project.org/ontology/disk#WorkflowBinding"/>
        <rdfs:range rdf:resource="http://www.wings-workflows.org/ontology/execution.owl#Execution"/>
        <rdfs:comment xml:lang="en">Property that links a workflow bundle to the execution results of the workflow</rdfs:comment>
        <rdfs:label xml:lang="en">has execution</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://disk-project.org/ontology/disk#hasHypothesis -->


    <owl:ObjectProperty rdf:about="http://disk-project.org/ontology/disk#hasHypothesis">
        <rdfs:domain rdf:resource="http://disk-project.org/ontology/disk#TriggeredLineOfInquiry"/>
        <rdfs:range rdf:resource="http://disk-project.org/ontology/disk#Hypothesis"/>
        <rdfs:comment xml:lang="en">property that links a triggered line of inquiry to the hypothesis that matched to it.</rdfs:comment>
        <rdfs:label xml:lang="en">has hypothesis</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://disk-project.org/ontology/disk#hasHypothesisVariable -->


    <owl:ObjectProperty rdf:about="http://disk-project.org/ontology/disk#hasHypothesisVariable">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://disk-project.org/ontology/disk#MetaWorkflow"/>
                    <rdf:Description rdf:about="http://disk-project.org/ontology/disk#WorkflowBinding"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://disk-project.org/ontology/disk#Variable"/>
                    <rdf:Description rdf:about="http://www.wings-workflows.org/ontology/workflow.owl#Variable"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">This property is used to identify which variables in the hypothesis are used in the workflows.</rdfs:comment>
        <rdfs:label xml:lang="en">has hypothesis variable</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://disk-project.org/ontology/disk#hasLineOfInquiry -->


    <owl:ObjectProperty rdf:about="http://disk-project.org/ontology/disk#hasLineOfInquiry">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/prov#wasDerivedFrom"/>
        <rdfs:domain rdf:resource="http://disk-project.org/ontology/disk#TriggeredLineOfInquiry"/>
        <rdfs:range rdf:resource="http://disk-project.org/ontology/disk#LineOfInquiry"/>
        <rdfs:comment xml:lang="en">Property that links a triggered line of inquiry to the original line of inquiry from which it was derived</rdfs:comment>
        <rdfs:label xml:lang="en">has line of inquiry</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://disk-project.org/ontology/disk#hasMetaWorkflowBinding -->


    <owl:ObjectProperty rdf:about="http://disk-project.org/ontology/disk#hasMetaWorkflowBinding">
        <rdfs:subPropertyOf rdf:resource="http://disk-project.org/ontology/disk#hasWorkflowBinding"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://disk-project.org/ontology/disk#LineOfInquiry"/>
                    <rdf:Description rdf:about="http://disk-project.org/ontology/disk#TriggeredLineOfInquiry"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://disk-project.org/ontology/disk#WorkflowBinding"/>
        <rdfs:comment xml:lang="en">The workflow bindings associated to a metaworkflow, i.e., a workflow and a variable binding</rdfs:comment>
        <rdfs:label xml:lang="en">has metaworkflow binding</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://disk-project.org/ontology/disk#hasOptionalParameter -->


    <owl:ObjectProperty rdf:about="http://disk-project.org/ontology/disk#hasOptionalParameter">
        <rdfs:domain rdf:resource="http://disk-project.org/ontology/disk#WorkflowBinding"/>
        <rdfs:range rdf:resource="http://disk-project.org/ontology/disk#VariableBinding"/>
        <rdfs:comment xml:lang="en">Property to store the optional parameters that can be inputted to a workflow</rdfs:comment>
        <rdfs:label xml:lang="en">has optional parameter</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://disk-project.org/ontology/disk#hasParameter -->


    <owl:ObjectProperty rdf:about="http://disk-project.org/ontology/disk#hasParameter">
        <rdfs:domain rdf:resource="http://disk-project.org/ontology/disk#WorkflowBinding"/>
        <rdfs:range rdf:resource="http://disk-project.org/ontology/disk#VariableBinding"/>
        <rdfs:comment xml:lang="en">Property to store the parameters to be inputted to a workflow</rdfs:comment>
        <rdfs:label xml:lang="en">has parameter</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://disk-project.org/ontology/disk#hasParentHypothesis -->


    <owl:ObjectProperty rdf:about="http://disk-project.org/ontology/disk#hasParentHypothesis">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/prov#wasRevisionOf"/>
        <rdfs:domain rdf:resource="http://disk-project.org/ontology/disk#Hypothesis"/>
        <rdfs:range rdf:resource="http://disk-project.org/ontology/disk#Hypothesis"/>
        <rdfs:comment xml:lang="en">Property that links to the parent hypothesis from which the current hypothesis has been derived</rdfs:comment>
        <rdfs:label xml:lang="en">has parent hypothesis</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://disk-project.org/ontology/disk#hasQuestion -->


    <owl:ObjectProperty rdf:about="http://disk-project.org/ontology/disk#hasQuestion">
        <rdfs:domain rdf:resource="http://disk-project.org/ontology/disk#LineOfInquiry"/>
        <rdfs:range rdf:resource="https://w3id.org/sqo#Question"/>
        <rdfs:comment xml:lang="en">Property that links a Line of Inquiry with a question</rdfs:comment>
        <rdfs:label xml:lang="en">has question</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://disk-project.org/ontology/disk#hasResultingHypothesis -->


    <owl:ObjectProperty rdf:about="http://disk-project.org/ontology/disk#hasResultingHypothesis">
        <rdfs:domain rdf:resource="http://disk-project.org/ontology/disk#TriggeredLineOfInquiry"/>
        <rdfs:range rdf:resource="http://disk-project.org/ontology/disk#Hypothesis"/>
        <rdfs:comment xml:lang="en">Property that links a triggered line of inquiry to the hypothesis revision that resulted from its analysis</rdfs:comment>
        <rdfs:label xml:lang="en">has resulting hypothesis</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://disk-project.org/ontology/disk#hasRevisedHypothesisVariable -->


    <owl:ObjectProperty rdf:about="http://disk-project.org/ontology/disk#hasRevisedHypothesisVariable">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://disk-project.org/ontology/disk#MetaWorkflow"/>
                    <rdf:Description rdf:about="http://disk-project.org/ontology/disk#WorkflowBinding"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://disk-project.org/ontology/disk#Variable"/>
                    <rdf:Description rdf:about="http://www.wings-workflows.org/ontology/workflow.owl#Variable"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="en">Property that links a metaworkflow to its revised hypothesis variable.</rdfs:comment>
        <rdfs:label xml:lang="en">has revised hypothesis variable</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://disk-project.org/ontology/disk#hasTriggeredLineOfInquiry -->


    <owl:ObjectProperty rdf:about="http://disk-project.org/ontology/disk#hasTriggeredLineOfInquiry">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/prov#has_provenance"/>
        <rdfs:domain rdf:resource="http://disk-project.org/ontology/disk#ConfidenceReport"/>
        <rdfs:range rdf:resource="http://disk-project.org/ontology/disk#TriggeredLineOfInquiry"/>
        <rdfs:comment xml:lang="en">Property that links a confidence report to the triggered line of inquiry that contains all the provenance for the report&apos;s value.</rdfs:comment>
        <rdfs:label xml:lang="en">has triggered line of inquiry</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://disk-project.org/ontology/disk#hasVariable -->


    <owl:ObjectProperty rdf:about="http://disk-project.org/ontology/disk#hasVariable">
        <rdfs:domain rdf:resource="http://disk-project.org/ontology/disk#VariableBinding"/>
        <rdfs:range rdf:resource="http://www.wings-workflows.org/ontology/workflow.owl#Variable"/>
        <rdfs:comment xml:lang="en">Property that links a variable binding to its corresponding variable</rdfs:comment>
        <rdfs:label xml:lang="en">has variable</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://disk-project.org/ontology/disk#hasVariableBinding -->


    <owl:ObjectProperty rdf:about="http://disk-project.org/ontology/disk#hasVariableBinding">
        <rdfs:domain rdf:resource="http://disk-project.org/ontology/disk#WorkflowBinding"/>
        <rdfs:range rdf:resource="http://disk-project.org/ontology/disk#VariableBinding"/>
        <rdfs:comment xml:lang="en">Property that links a Workflow binding to it correspondent variable binding</rdfs:comment>
        <rdfs:label xml:lang="en">has variable binding</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://disk-project.org/ontology/disk#hasWorkflow -->


    <owl:ObjectProperty rdf:about="http://disk-project.org/ontology/disk#hasWorkflow">
        <rdfs:domain rdf:resource="http://disk-project.org/ontology/disk#WorkflowBinding"/>
        <rdfs:range rdf:resource="http://www.wings-workflows.org/ontology/workflow.owl#Workflow"/>
        <rdfs:comment xml:lang="en">Property that links a Workflow binding to its associated workflow</rdfs:comment>
        <rdfs:label xml:lang="en">has workflow</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://disk-project.org/ontology/disk#hasWorkflowBinding -->


    <owl:ObjectProperty rdf:about="http://disk-project.org/ontology/disk#hasWorkflowBinding">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://disk-project.org/ontology/disk#LineOfInquiry"/>
                    <rdf:Description rdf:about="http://disk-project.org/ontology/disk#TriggeredLineOfInquiry"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://disk-project.org/ontology/disk#WorkflowBinding"/>
        <rdfs:comment xml:lang="en">Property that links a line of inquiry or executed line of inquiry to the workflow bindings present on it</rdfs:comment>
        <rdfs:label xml:lang="en">has workflow bindings</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/prov#has_provenance -->


    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/prov#has_provenance">
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/prov#"/>
        <rdfs:label xml:lang="en">has provenance</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/prov#wasDerivedFrom -->


    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/prov#wasDerivedFrom">
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/prov#"/>
        <rdfs:label xml:lang="en">was derived from</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/prov#wasRevisionOf -->


    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/prov#wasRevisionOf">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/prov#wasDerivedFrom"/>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/ns/prov#"/>
        <rdfs:label xml:lang="en">was revision of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://disk-project.org/ontology/disk#dataQueryDescription -->


    <owl:DatatypeProperty rdf:about="http://disk-project.org/ontology/disk#dataQueryDescription">
        <rdfs:domain rdf:resource="http://disk-project.org/ontology/disk#LineOfInquiry"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Text description of this line of inquiry data query</rdfs:comment>
        <rdfs:label xml:lang="en">has query description</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://disk-project.org/ontology/disk#dateCreated -->


    <owl:DatatypeProperty rdf:about="http://disk-project.org/ontology/disk#dateCreated">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://disk-project.org/ontology/disk#Hypothesis"/>
                    <rdf:Description rdf:about="http://disk-project.org/ontology/disk#LineOfInquiry"/>
                    <rdf:Description rdf:about="http://disk-project.org/ontology/disk#TriggeredLineOfInquiry"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
        <rdfs:comment xml:lang="en">Property which describes when the hypothesis was created</rdfs:comment>
        <rdfs:label xml:lang="en">date created</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://disk-project.org/ontology/disk#dateModified -->


    <owl:DatatypeProperty rdf:about="http://disk-project.org/ontology/disk#dateModified">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://disk-project.org/ontology/disk#Hypothesis"/>
                    <rdf:Description rdf:about="http://disk-project.org/ontology/disk#LineOfInquiry"/>
                    <rdf:Description rdf:about="http://disk-project.org/ontology/disk#TriggeredLineOfInquiry"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
        <rdfs:comment xml:lang="en">Property which describes when the hypothesis was modified</rdfs:comment>
        <rdfs:label xml:lang="en">date modified</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://disk-project.org/ontology/disk#hasBindingValue -->


    <owl:DatatypeProperty rdf:about="http://disk-project.org/ontology/disk#hasBindingValue">
        <rdfs:domain rdf:resource="http://disk-project.org/ontology/disk#VariableBinding"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Property that links a binding to its associated value</rdfs:comment>
        <rdfs:label xml:lang="en">has binding value</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://disk-project.org/ontology/disk#hasConfidenceValue -->


    <owl:DatatypeProperty rdf:about="http://disk-project.org/ontology/disk#hasConfidenceValue">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://disk-project.org/ontology/disk#ConfidenceReport"/>
                    <rdf:Description rdf:about="http://disk-project.org/ontology/disk#TriggeredLineOfInquiry"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
        <rdfs:comment xml:lang="en">Property that binds a resource to its confidence value</rdfs:comment>
        <rdfs:label xml:lang="en">has confidence value</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://disk-project.org/ontology/disk#hasDataQuery -->


    <owl:DatatypeProperty rdf:about="http://disk-project.org/ontology/disk#hasDataQuery">
        <rdfs:domain rdf:resource="http://disk-project.org/ontology/disk#LineOfInquiry"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Property that links a line of inquiry to the data query text associated to it</rdfs:comment>
        <rdfs:label xml:lang="en">has data query</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://disk-project.org/ontology/disk#hasDataQueryDescription -->


    <owl:DatatypeProperty rdf:about="http://disk-project.org/ontology/disk#hasDataQueryDescription">
        <rdfs:domain rdf:resource="http://disk-project.org/ontology/disk#LineOfInquiry"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Property to describe the data retrieved from a data query in a Line of Inquiry.</rdfs:comment>
        <rdfs:label xml:lang="en">Data Query Description</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://disk-project.org/ontology/disk#hasDataSource -->


    <owl:DatatypeProperty rdf:about="http://disk-project.org/ontology/disk#hasDataSource">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://disk-project.org/ontology/disk#LineOfInquiry"/>
                    <rdf:Description rdf:about="http://disk-project.org/ontology/disk#TriggeredLineOfInquiry"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Property to configure the desired target wiki for the data query in a Line of Inquiry.</rdfs:comment>
        <rdfs:label xml:lang="en">has data source</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://disk-project.org/ontology/disk#hasEmail -->


    <owl:DatatypeProperty rdf:about="http://disk-project.org/ontology/disk#hasEmail">
        <rdfs:domain rdf:resource="http://disk-project.org/ontology/disk#Person"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Email of a person</rdfs:comment>
        <rdfs:label xml:lang="en">has email</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://disk-project.org/ontology/disk#hasHypothesisQuery -->


    <owl:DatatypeProperty rdf:about="http://disk-project.org/ontology/disk#hasHypothesisQuery">
        <rdfs:domain rdf:resource="http://disk-project.org/ontology/disk#LineOfInquiry"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">Property that links a line of inquiry to the hypothesis query text</rdfs:comment>
        <rdfs:label xml:lang="en">has hypothesis query</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://disk-project.org/ontology/disk#hasId -->


