        <rdfs:comment>Inner step of a execution</rdfs:comment>
        <rdfs:label>has step</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://disk-project.org/ontology/disk#hasWorkflow -->


    <owl:ObjectProperty rdf:about="http://disk-project.org/ontology/disk#hasWorkflow">
        <rdfs:domain rdf:resource="http://disk-project.org/ontology/disk#WorkflowSeed"/>
        <rdfs:range rdf:resource="http://disk-project.org/ontology/disk#Workflow"/>
        <rdfs:comment xml:lang="en">Property that links a Workflow binding to its associated workflow</rdfs:comment>
        <rdfs:label xml:lang="en">has workflow</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://disk-project.org/ontology/disk#hasWorkflowInstantiation -->


    <owl:ObjectProperty rdf:about="http://disk-project.org/ontology/disk#hasWorkflowInstantiation">
        <rdfs:domain rdf:resource="http://disk-project.org/ontology/disk#TriggeredLineOfInquiry"/>
        <rdfs:range rdf:resource="http://disk-project.org/ontology/disk#WorkflowInstantiation"/>
        <rdfs:comment>Property that links a triggered line of inquiry with a workflow instantiation</rdfs:comment>
        <rdfs:label>has workflow instantiation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://disk-project.org/ontology/disk#hasWorkflowSeed -->


    <owl:ObjectProperty rdf:about="http://disk-project.org/ontology/disk#hasWorkflowSeed">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://disk-project.org/ontology/disk#LineOfInquiry"/>
                    <rdf:Description rdf:about="http://disk-project.org/ontology/disk#TriggeredLineOfInquiry"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://disk-project.org/ontology/disk#WorkflowSeed"/>
        <rdfs:comment>Property that links a line of inquiry with a workflow seed</rdfs:comment>
        <rdfs:label>has workflow seed</rdfs:label>
    </owl:ObjectProperty>
    


