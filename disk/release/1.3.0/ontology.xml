                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://w3id.org/sqo#Question"/>
        <rdfs:comment xml:lang="en">Property that links a Line of Inquiry with a question</rdfs:comment>
        <rdfs:label xml:lang="en">has question</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://disk-project.org/ontology/disk#hasQuestionBindings -->


    <owl:ObjectProperty rdf:about="http://disk-project.org/ontology/disk#hasQuestionBindings">
        <rdfs:domain rdf:resource="http://disk-project.org/ontology/disk#Goal"/>
        <rdfs:range rdf:resource="http://disk-project.org/ontology/disk#VariableBinding"/>
        <rdfs:comment>Variable assignations choose for the user on Goal creation</rdfs:comment>
        <rdfs:label>has question bindings</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://disk-project.org/ontology/disk#hasResult -->


    <owl:ObjectProperty rdf:about="http://disk-project.org/ontology/disk#hasResult">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://disk-project.org/ontology/disk#TriggeredLineOfInquiry"/>
                    <rdf:Description rdf:about="http://www.wings-workflows.org/ontology/execution.owl#Execution"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://disk-project.org/ontology/disk#GoalResult"/>
        <rdfs:comment>Results generated from an execution and related to a triggered line of inquiry</rdfs:comment>
        <rdfs:label>has result</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://disk-project.org/ontology/disk#hasSource -->


    <owl:ObjectProperty rdf:about="http://disk-project.org/ontology/disk#hasSource">
        <rdfs:comment>Endpoint used as source API for this resource</rdfs:comment>
        <rdfs:label>has source</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://disk-project.org/ontology/disk#hasStep -->


    <owl:ObjectProperty rdf:about="http://disk-project.org/ontology/disk#hasStep">
