    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://disk-project.org/ontology/disk#createdFrom -->


    <owl:ObjectProperty rdf:about="http://disk-project.org/ontology/disk#createdFrom">
        <rdfs:domain rdf:resource="http://disk-project.org/ontology/disk#WorkflowInstantiation"/>
        <rdfs:range rdf:resource="http://disk-project.org/ontology/disk#WorkflowSeed"/>
        <rdfs:comment>Workflow seed from which this workflow instantiation was derived</rdfs:comment>
        <rdfs:label>created from</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://disk-project.org/ontology/disk#hasAuthor -->


    <owl:ObjectProperty rdf:about="http://disk-project.org/ontology/disk#hasAuthor">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://disk-project.org/ontology/disk#Goal"/>
                    <rdf:Description rdf:about="http://disk-project.org/ontology/disk#Hypothesis"/>
                    <rdf:Description rdf:about="http://disk-project.org/ontology/disk#LineOfInquiry"/>
                    <rdf:Description rdf:about="http://disk-project.org/ontology/disk#TriggeredLineOfInquiry"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://disk-project.org/ontology/disk#Entity"/>
        <rdfs:comment xml:lang="en">Property to describe the author of a Line of Inquiry, Triggered Line of Inquiry or Goal</rdfs:comment>
        <rdfs:label xml:lang="en">has author</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://disk-project.org/ontology/disk#hasDataBindings -->


    <owl:ObjectProperty rdf:about="http://disk-project.org/ontology/disk#hasDataBindings">
        <rdfs:domain rdf:resource="http://disk-project.org/ontology/disk#WorkflowInstantiation"/>
        <rdfs:range rdf:resource="http://disk-project.org/ontology/disk#VariableBinding"/>
        <rdfs:comment>Property to store the data and files to be inputted to a workflow. This data is generated after data query execution.</rdfs:comment>
        <rdfs:label>has data bindings</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://disk-project.org/ontology/disk#hasDataQuery -->


    <owl:ObjectProperty rdf:about="http://disk-project.org/ontology/disk#hasDataQuery">
        <rdfs:domain rdf:resource="http://disk-project.org/ontology/disk#LineOfInquiry"/>
        <rdfs:range rdf:resource="http://disk-project.org/ontology/disk#DataQueryTemplate"/>
        <rdfs:comment>Property that links a line of inquiry to the data query associated to it</rdfs:comment>
        <rdfs:label>has data query</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://disk-project.org/ontology/disk#hasDataQueryResult -->


    <owl:ObjectProperty rdf:about="http://disk-project.org/ontology/disk#hasDataQueryResult">
        <rdfs:domain rdf:resource="http://disk-project.org/ontology/disk#TriggeredLineOfInquiry"/>
        <rdfs:range rdf:resource="http://disk-project.org/ontology/disk#DataQueryResults"/>
        <rdfs:comment>Raw result of executing a data query</rdfs:comment>
        <rdfs:label>has data query result</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://disk-project.org/ontology/disk#hasDataSource -->


    <owl:ObjectProperty rdf:about="http://disk-project.org/ontology/disk#hasDataSource">
        <rdfs:subPropertyOf rdf:resource="http://disk-project.org/ontology/disk#hasSource"/>
        <rdfs:domain rdf:resource="http://disk-project.org/ontology/disk#DataQueryTemplate"/>
        <rdfs:range rdf:resource="http://disk-project.org/ontology/disk#Endpoint"/>
        <rdfs:comment>Endpoint used as data source</rdfs:comment>
        <rdfs:label>has data source</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://disk-project.org/ontology/disk#hasExecution -->


    <owl:ObjectProperty rdf:about="http://disk-project.org/ontology/disk#hasExecution">
        <rdfs:domain rdf:resource="http://disk-project.org/ontology/disk#WorkflowInstantiation"/>
        <rdfs:range rdf:resource="http://disk-project.org/ontology/disk#Execution"/>
        <rdfs:comment xml:lang="en">Property that links a workflow bundle to the execution results of the workflow</rdfs:comment>
        <rdfs:label xml:lang="en">has execution</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://disk-project.org/ontology/disk#hasGoal -->


    <owl:ObjectProperty rdf:about="http://disk-project.org/ontology/disk#hasGoal">
        <rdfs:domain rdf:resource="http://disk-project.org/ontology/disk#TriggeredLineOfInquiry"/>
        <rdfs:range rdf:resource="http://disk-project.org/ontology/disk#Goal"/>
        <rdfs:comment>Property that links a triggered line of inquiry with its goal</rdfs:comment>
        <rdfs:label>has goal</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://disk-project.org/ontology/disk#hasHypothesis -->


    <owl:ObjectProperty rdf:about="http://disk-project.org/ontology/disk#hasHypothesis">
        <rdfs:domain rdf:resource="http://disk-project.org/ontology/disk#TriggeredLineOfInquiry"/>
        <rdfs:range rdf:resource="http://disk-project.org/ontology/disk#Hypothesis"/>
        <rdfs:comment xml:lang="en">property that links a triggered line of inquiry to the hypothesis that matched to it.</rdfs:comment>
        <rdfs:label xml:lang="en">has hypothesis</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://disk-project.org/ontology/disk#hasInput -->


    <owl:ObjectProperty rdf:about="http://disk-project.org/ontology/disk#hasInput">
        <rdfs:domain rdf:resource="http://disk-project.org/ontology/disk#WorkflowSeed"/>
        <rdfs:range rdf:resource="http://disk-project.org/ontology/disk#VariableBinding"/>
        <rdfs:comment>Input data assignation for a workflow seed</rdfs:comment>
        <rdfs:label>has input files</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://disk-project.org/ontology/disk#hasInputFile -->


    <owl:ObjectProperty rdf:about="http://disk-project.org/ontology/disk#hasInputFile">
        <rdfs:domain rdf:resource="http://disk-project.org/ontology/disk#Execution"/>
        <rdfs:range rdf:resource="http://disk-project.org/ontology/disk#VariableBinding"/>
        <rdfs:comment>Input files created from the execution of a workflow seed</rdfs:comment>
        <rdfs:label>has input file</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://disk-project.org/ontology/disk#hasLineOfInquiry -->


    <owl:ObjectProperty rdf:about="http://disk-project.org/ontology/disk#hasLineOfInquiry">
        <rdfs:domain rdf:resource="http://disk-project.org/ontology/disk#TriggeredLineOfInquiry"/>
        <rdfs:range rdf:resource="http://disk-project.org/ontology/disk#LineOfInquiry"/>
        <rdfs:comment xml:lang="en">Property that links a triggered line of inquiry to the original line of inquiry from which it was derived</rdfs:comment>
        <rdfs:label xml:lang="en">has line of inquiry</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://disk-project.org/ontology/disk#hasMetaWorkflowInstantiation -->


    <owl:ObjectProperty rdf:about="http://disk-project.org/ontology/disk#hasMetaWorkflowInstantiation">
        <rdfs:subPropertyOf rdf:resource="http://disk-project.org/ontology/disk#hasWorkflowInstantiation"/>
        <rdfs:domain rdf:resource="http://disk-project.org/ontology/disk#TriggeredLineOfInquiry"/>
        <rdfs:range rdf:resource="http://disk-project.org/ontology/disk#WorkflowInstantiation"/>
        <rdfs:comment>Property that links a triggered line of inquiry with a meta-workflow instantiation</rdfs:comment>
        <rdfs:label>has metaworkflow instantiation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://disk-project.org/ontology/disk#hasMetaWorkflowSeed -->


    <owl:ObjectProperty rdf:about="http://disk-project.org/ontology/disk#hasMetaWorkflowSeed">
        <rdfs:subPropertyOf rdf:resource="http://disk-project.org/ontology/disk#hasWorkflowSeed"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://disk-project.org/ontology/disk#LineOfInquiry"/>
                    <rdf:Description rdf:about="http://disk-project.org/ontology/disk#TriggeredLineOfInquiry"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://disk-project.org/ontology/disk#WorkflowSeed"/>
        <rdfs:comment>Property that links a line of inquiry with a meta workflow seed</rdfs:comment>
        <rdfs:label>has metaworkflow seed</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://disk-project.org/ontology/disk#hasOutputFile -->


    <owl:ObjectProperty rdf:about="http://disk-project.org/ontology/disk#hasOutputFile">
        <rdfs:domain rdf:resource="http://disk-project.org/ontology/disk#Execution"/>
        <rdfs:range rdf:resource="http://disk-project.org/ontology/disk#VariableBinding"/>
        <rdfs:comment>Output files created from the execution of a workflow seed</rdfs:comment>
        <rdfs:label>has output file</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://disk-project.org/ontology/disk#hasParameter -->


    <owl:ObjectProperty rdf:about="http://disk-project.org/ontology/disk#hasParameter">
        <rdfs:domain rdf:resource="http://disk-project.org/ontology/disk#WorkflowSeed"/>
        <rdfs:range rdf:resource="http://disk-project.org/ontology/disk#VariableBinding"/>
        <rdfs:comment xml:lang="en">Parameter assignation to be inputted in a workflow seed</rdfs:comment>
        <rdfs:label xml:lang="en">has parameter</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://disk-project.org/ontology/disk#hasQuestion -->


    <owl:ObjectProperty rdf:about="http://disk-project.org/ontology/disk#hasQuestion">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://disk-project.org/ontology/disk#Hypothesis"/>
